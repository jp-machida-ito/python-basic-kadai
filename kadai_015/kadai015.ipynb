        "#課題\n",
        "class Human:\n",
        "\n",
        "#初期化\n",
        "  def __init__(self,name,age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "#情報開示の為のメソッド\n",
        "  def printinfo(self):\n",
        "    print(f\"{self.name}、{self.age}\")\n",
        "\n",
        "user = Human(\"太郎\",30)\n",
        "\n",
        "user.printinfo()\n"
