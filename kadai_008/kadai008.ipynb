        "##課題8\n",
        "import random\n",
        "\n",
        "#変数varへ代入\n",
        "var=random.randint(1,30)\n",
        "\n",
        "print(var)\n",
        "\n",
        "#条件分岐\n",
        "if var % 3 == 0 and var % 5 ==0:\n",
        "  print(\"FizzBuzz\")\n",
        "\n",
        "elif var % 3 == 0:\n",
        "  print(\"Fizz\")\n",
        "\n",
        "elif var % 5 == 0:\n",
        "  print(\"Buzz\")\n",
        "\n",
        "else:\n",
        "  print(var)\n",
        "\n"
